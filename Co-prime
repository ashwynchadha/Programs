//FIND HCF, AND THEN FIND IF THE 2 NUMBERS ARE COPRIME OR NOT
#include<iostream.h>
int coprime(int,int);
void main()
{
	int num1,num2,HCF=1;
	cout<<"This will find if 2 integers are co-prime or not.\n";
	cout<<"Enter the integers:-\n";
	cin>>num1>>num2;
	cout<<endl<<endl;

	HCF=coprime(num1,num2);

	cout<<"HCF is = "<<HCF<<endl;
	if(HCF==1)
	cout<<"Therefore, the 2 numbers are co-prime.\n";
	else
	cout<<"Hence, the 2 numbers are not co-prime.\n";
}



	int coprime(int num1,int num2)
	{
	int HCF;
	for(int i=1;i<=num1,i<=num2;i++)
	if(num1%i==0 && num2%i==0)
	HCF=i;
	return HCF;
	}
