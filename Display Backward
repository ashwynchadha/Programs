#include<iostream.h>
void main()
{
	int n,a[100];
	cout<<"This program will display all the numbers that you input backward.\n";
	cout<<"How many numbers would you like to display?\n";
	cin>>n;
	cout<<"Enter the numbers in quick succession, each followed by enter.\n";

	for (int i=0;i<n;i++)
	cin>>a[i];

	cout<<"\n\n\n\nThe sequence backward is -\n";

	for (i=(n-1);i>=0;i--)
	cout<<a[i]<<endl;
}
